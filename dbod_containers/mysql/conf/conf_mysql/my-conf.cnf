[mysqld]
server-id = 1
port=5500
general-log-file = /ORA/dbs03/TEST/mysql/mysql.log
socket = /var/lib/mysql/mysql.sock.TEST.5500
datadir = /ORA/dbs03/TEST/mysql
log-bin = /ORA/dbs02/TEST/mysql/binlog
